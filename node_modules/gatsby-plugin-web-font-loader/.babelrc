{
	"presets": [
		"env",
		"stage-0",
		"react"
	],
	"plugins": [
		[
			"styled-jsx/babel",
			{
				"plugins": [
					"styled-jsx-plugin-postcss"
				]
			}
		],
		"transform-es2015-modules-commonjs",
		"syntax-dynamic-import",
		"dynamic-import-node",
		[
			"transform-runtime",
			{
				"polyfill": false,
				"regenerator": true
			}
		]
	]
}
